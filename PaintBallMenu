local lp = game:GetService('Players').LocalPlayer
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

if game.PlaceId == 3527629287 then

    local Window = OrionLib:MakeWindow({Name = "OP | Big Paintball", HidePremium = true, IntroText = "Mady By BA3981", SaveConfig = true, ConfigFolder = "BIGPaintballConfig"})

    -- [TABS] --

    local MainTab = Window:MakeTab({
        Name = "Main",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    local VisualsTab = Window:MakeTab({
        Name = "Visuals",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    -- [BUTTONS] --

    MainTab:AddButton({
        Name = "GunMods",
        Callback = function()
                for i, v in next, getgc(true) do
                    if type(v) == 'table' and rawget(v, 'damage') then
                        v.shotrate = 0.0001
                        v.automatic = true
                        v.damage = math.huge
                        v.velocity = 500
                    end
                end
          end    
    })

    -- [TOGGLES] --

    MainTab:AddToggle({
        Name = "Aimbot",
        Default = false,
        Callback = function(Value)
            getgenv().Aimbot = true
        end    
    })

    Tab:AddToggle({
        Name = "Tracers",
        Default = false,
        Callback = function(Value)
            getgenv().Tracers = true
        end    
    })

        OrionLib:MakeNotification({
            Name = "OP | Big Paintball",
            Content = "Successfully Loaded.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
else
    OrionLib:MakeNotification({
        Name = "OP | Big Paintball",
        Content = "This is not Big Paintball.",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end
OrionLib:Init()
